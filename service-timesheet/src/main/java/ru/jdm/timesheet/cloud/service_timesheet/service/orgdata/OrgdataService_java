package ru.jdm.timesheet.cloud.service_orgdata.service.orgdata;

import ru.jdm.timesheet.cloud.service_orgdata.entity.orgdata.Orgdata;

import java.util.List;


/**
 *=Orgdata Service Interface
 */
public interface OrgdataService {
    //--get all records
    List<Orgdata> findAll();

    //--get single record by id
    Orgdata findById(Long theId);

    //--add/save/update single record
    void save(Orgdata theOrgdata);

    //--delete single record by id
    void deleteById(Long theId);
}
